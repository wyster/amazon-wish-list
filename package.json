{
  "name": "amazon-wish-list",
  "version": "0.5.3",
  "description": "A JavaScript scraper to look up public amazon wish lists by customer or wish list id.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && npx babel src --out-dir lib",
    "build::watch": "npm run clean && npx babel src --out-dir lib --watch",
    "test": "npm run build && npx babel-node ./tests/index.js",
    "lint": "npx eslint src",
    "lint::fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "preversion": "npm test"
  },
  "author": "Chris Landa <stylesuxx@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^0.22.0",
    "request": "^2.83.0",
    "request-debug": "^0.2.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^3.5.0",
    "tape": "^4.2.2",
    "debug": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stylesuxx/amazon-wish-list"
  },
  "keywords": [
    "amazon wish list",
    "amazon wishlist",
    "amazon",
    "wish list",
    "wishlist",
    "api"
  ],
  "engines": {
    "node": ">=4.5.0"
  }
}
